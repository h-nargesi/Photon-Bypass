{
    "Serilog": {
        "WriteTo": [
            {
                "Name": "File",
                "Args": {
                    "path": "events-.log",
                    "restrictedToMinimumLevel": "Debug",
                    "rollingInterval": "Day",
                    "rollOnFileSizeLimit": true,
                    "buffered": true
                }
            }
        ]
    },
    "AllowedHosts": "*"
}
